<?xml version="1.0" encoding="UTF-8"?>
<actions>
  <action>
    <actionName>run</actionName>
    <packagings>
      <packaging>*</packaging>
    </packagings>
    <goals>
      <goal>spring-boot:run</goal>
    </goals>
    <properties>
      <spring-boot.run.jvmArguments>-Dfile.encoding=UTF-8 -Dspring.output.ansi.enabled=always -Dserver.tomcat.basedir=target -noverify -XX:TieredStopAtLevel=1</spring-boot.run.jvmArguments>
    </properties>
  </action>
</actions>
